#include<iostream>
#include<cstdio>
#include<cstring>
#include<cstdlib>
#include<climits>
#include<algorithm>
#include<vector>
#include<stack>
#include<set>
#include<queue>
#include<map>
#include<cmath>
#include<sstream>
#include<string>

typedef long long ll;
typedef unsigned long long ull;
using namespace std;

int xd[1005][1005];
int xdt[1005][1005];
int dp[1005],a[1005];

int main(){
	int n,k;
	cin>>n>>k;
	for(int i=1;i<=n;i++)
		cin>>a[i];
	//zuo:-1 you:1
	for(int i=1;i<=n;i++){
		for(int j=1;j<i;j++){
			dp[a[i]][a[j]]=j+1;
		}
	}
	for(int i=1;i<k;i++){
		for(int i=1;i<=n;i++)
			cin>>a[i];
		for(int i=1;i<=n;i++)
			for(int j=i+1;j<=n;j++)
				if(dp[a[i]][a[j]]>0)
					dp[a[i]][a[j]]=0;
	}
	int ans=0;
	for(int i=1;i<=n;i++)
		ans=max(ans,dp[i]);
	cout<<ans;
	return 0;
}
