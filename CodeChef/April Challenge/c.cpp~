#include<iostream>
#include<cstring>
#include<cstdio>
using namespace std;
int s[100005];
char so[100005];
int num[100005][10];
int main()
{
	int n,m;
	cin>>n>>m;
	int t;
	char c;
	scanf("%s",so);
	for(int i=0;i<strlen(so);i++)
		s[i+1]=so[i]-'0';
	for(int i=1;i<=n;i++)
	{
		num[i][s[i]]++;
		for(int j=0;j<10;i++)
			num[i+1][j]=num[i][j];
	}
	while(m--)
	{
		int x;
		cin>>x;
		int sum=0;
		int a=s[x];
		for(int i=0;i<10;i++)
		{
			if(i<a)
				sum+=num[x][i]*(a-i);
			if(i>a)
				sum+=num[x][i]*(i-a);
		}
		cout<<sum<<endl;
	}
	return 0;
}
