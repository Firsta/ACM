#include<iostream>
#include<cstdio>
#include<cstring>
#include<cstdlib>
#include<climits>
#include<algorithm>
#include<vector>
#include<stack>
#include<set>
#include<queue>
#include<map>
#include<cmath>
#include<sstream>
#include<string>
#define INF 10000007

typedef long long ll;
typedef unsigned long long ull;
using namespace std;

ll a[100][100];

int main(){
	int n,m;
	scanf("%d%d",&n,&m);
	for(int i=1;i<=n;i++)
		scanf("%d",&a[i][0]);
	memset(a,0,sizeof(a));
	a[0][1]=233;
	for(int i=0;i<=n;i++){
		for(int j=0;j<=m;j++)
			cout<<a[i][j]<<' ';
		cout<<endl;
	}
	for(int i=2;i<=m;i++){
		a[0][i]=(a[0][i-1]%INF*(10%INF)%INF+3)%INF;
		a[0][i]%=INF;
	}
	for(int i=0;i<=n;i++){
		for(int j=0;j<=m;j++)
			cout<<a[i][j]<<' ';
		cout<<endl;
	}
	for(int i=1;i<=n;i++)
		for(int j=1;j<=m;j++)
			a[i][j]=(a[i-1][j]%INF+a[i][j-1]%INF)%INF;
	for(int i=0;i<=n;i++){
		for(int j=0;j<=m;j++)
			cout<<a[i][j]<<' ';
		cout<<endl;
	}
	printf("%d",a[n][m]);
	for(int i=0;i<=n;i++){
		for(int j=0;j<=m;j++)
			cout<<a[i][j]<<' ';
		cout<<endl;
	}
	return 0;
}
