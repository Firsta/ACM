#include<iostream>
#include<cstdio>
#include<cstring>
#include<cstdlib>
#include<climits>
#include<algorithm>
#include<vector>
#include<stack>
#include<set>
#include<queue>
#include<map>
#include<cmath>
#include<sstream>
#include<string>
#define INF 1000000

int dp[505][505];
int grid[505][505];

typedef long long ll;
using namespace std;

int main(){
	int t;
	scanf("%d",&t);
	while(t--){
		int r,c;
		scanf("%d%d",&r,&c);
		for(int i=1;i<=r;i++)
			for(int j=1;j<=c;j++)
				scanf("%d",&grid[i][j]);
		for(int i=1;i<=r;i++)
			for(int j=1;j<=c;j++)
				dp[i][j]=-INF;
		dp[r][c]=0;
		for(int i=r;i>=1;i--)
			for(int j=c;j>=1;j--){
				if(i+1<=r){
					dp[i][j]=max(dp[i][j],dp[i+1][j]);
				}
				if(j+1<=c){
					dp[i][j]=max(dp[i][j],dp[i][j+1]);
				}
				dp[i][j]+=grid[i][j];
			}
		for(int i=1;i<=r;i++){
			for(int j=1;j<=c;j++)
				cout<<dp[i][j]<<' ';
			cout<<endl;
		}
		cout<<-dp[1][1]+1<<endl;
	}
	return 0;
}
