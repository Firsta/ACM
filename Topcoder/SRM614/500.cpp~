#include<algorithm>
#include<vector>
#include<climits>
using namespace std;
class MinimumSquareEasy
{
	public:
		long long minArea(vector<int> x,vector<int> y)
		{
			long long area=LLONG_MAX;
			long long xmin=LLONG_MIN,xmax=LLONG_MAX,ymin=LLONG_MIN,ymax=LLONG_MAX;
			for(int i=0;i<x.size();i++)
				for(int j=0;j<x.size() && i!=j;j++)
				{
					for(int k=0;k<x.size();k++)
					{
						if(k==i||k==j)
							continue;
						xmin=min(xmin,long long (x[k]));
						xmax=max(xmax,long long (x[k]));
						ymin=min(ymin,long long (y[k]));
						ymax=max(ymax,long long (y[k]));
					}
					long long l=max(xmax-xmin,ymax-ymin)+2;
					area=min(area,l*l);
				}
			return area;
		}
};
